{
  "name": "flaru-player",
  "displayName": "Flaru Player",
  "version": "1.8.0",
  "versionCode": 180,
  "versionDate": "2022-10-09",
  "ruffleVersionDate": "2022-10-08",
  "description": "Flaru Flash Player",
  "main": "dist/main/index.js",
  "private": true,
  "homepage": "https://github.com/jooy2/flaru",
  "author": "jooy2 <jootc.help@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false GENERATE_SOURCEMAP=false NODE_ENV=production vite build && electron-builder -wml",
    "build:mac": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false GENERATE_SOURCEMAP=false NODE_ENV=production vite build && electron-builder --mac",
    "build:win": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production vite build && electron-builder --windows nsis:x64",
    "build:win-portable": "cross-env NODE_ENV=production vite build && electron-builder --windows portable --x64",
    "build:linux": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production vite build && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "format": "prettier .",
    "format:fix": "prettier . --write"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "cross-env": "^7.0.3",
    "electron": "^21.2.3",
    "electron-builder": "^23.6.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "vite": "^3.2.3",
    "vite-plugin-electron": "0.10.2",
    "vite-plugin-electron-renderer": "^0.10.3"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.13",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^8.1.0",
    "i18next": "^22.0.4",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
