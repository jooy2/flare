{
  "name": "open-ruffle-player",
  "version": "1.3.0",
  "versionCode": 130,
  "versionDate": "2022-03-26",
  "description": "OpenRuffle Flash Player",
  "main": "app/index.js",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "start-interface": "cross-env PORT=9090 BROWSER=none npm run start",
    "start-electron": "electron .",
    "start-electron-after-interface": "wait-on http://localhost:9090 && npm run start-electron",
    "build:mac": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false GENERATE_SOURCEMAP=false NODE_ENV=production react-scripts build && electron-builder --mac",
    "build:linux": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production react-scripts build && electron-builder --linux",
    "build:win": "cross-env GENERATE_SOURCEMAP=false NODE_ENV=production react-scripts build && electron-builder --windows nsis:x64",
    "build:win-portable": "react-scripts build && electron-builder --windows portable --x64",
    "dev": "cross-env NODE_ENV=dev concurrently -n interface,app 'npm:start-interface' 'npm:start-electron-after-interface'",
    "postinstall": "electron-builder install-app-deps",
    "lint:fix": "eslint --fix src app --ext .js,.jsx"
  },
  "build": {
    "fileAssociations": [
      {
        "ext": "swf",
        "name": "SWF File",
        "role": "Viewer"
      }
    ],
    "productName": "OpenRuffle Flash Player",
    "appId": "com.jooy2.openruffleplayer",
    "asar": true,
    "compression": "maximum",
    "extends": null,
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./resources/installer/icon.ico"
    },
    "nsis": {
      "oneClick": true
    },
    "linux": {
      "icon": "resources/installer",
      "target": [
        "deb",
        "rpm"
      ]
    },
    "mac": {
      "identity": null
    },
    "directories": {
      "buildResources": "./resources/installer/",
      "output": "./dist/"
    },
    "files": [
      "app/**/*",
      "config/**/*",
      "build/**/*"
    ]
  },
  "author": "jooy2 <jootc.help@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.2.0",
    "electron-builder": "^22.14.13",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react-scripts": "^5.0.0",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^8.0.1",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.4",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.16.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
