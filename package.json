  "version": "1.13.5",
  "versionCode": 1135,
  "versionDate": "2023-11-11",
  "ruffleVersionDate": "2023-11-11",
  "description": "Flaru SWF Flash Player",
  "main": "dist/main/index.js",
  "private": true,
  "homepage": "https://github.com/jooy2/flaru",
  "author": "jooy2 <jooy2.contact@gmail.com>",
